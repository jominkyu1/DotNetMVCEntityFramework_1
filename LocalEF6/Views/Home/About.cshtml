@model IEnumerable<LocalEF6.ViewModels.EnrollmentDateGroup>

@{
    ViewBag.Title = "About";
}
<main aria-labelledby="title">
    <h2>Students stats</h2>
    <table>
        <tr>
            <th>Enrollment Date</th>
            <th>Students</th>
        </tr>
        
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.EnrollmentDate)</td>
                <td>@Html.DisplayFor(modelItem => item.StudentCount)</td>
            </tr>
        }
    </table>
</main>